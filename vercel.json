{
    "version": 2,
    "installCommand": "npm install && npx prisma generate",
    "builds": [
        {
            "src": "server/index.ts",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}